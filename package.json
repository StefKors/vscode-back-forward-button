{
  "name": "vscode-back-forward-button",
  "displayName": "Back and Forward buttons",
  "description": "",
  "version": "0.1.3",
  "icon": "icon.png",
  "publisher": "grimmer",
  "engines": {
    "vscode": "^1.17.0"
  },
  "repository": {
    "url": "https://github.com/grimmer0125/vscode-back-forward-button"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "*"
  ],
  "main": "./out/extension",
  "contributes": {
    "commands": [],
    "configuration": {
      "title": "Back and Forward buttons",
      "properties": {
        "backAndForwardButtons.backText": {
          "type": "string",
          "default": "<",
          "description": "The text to use for the back button"
        },
        "backAndForwardButtons.forwardText": {
          "type": "string",
          "default": ">",
          "description": "The text to use for the forward button"
        }
      }
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "postinstall": "node ./node_modules/vscode/bin/install",
    "test": "npm run compile && node ./node_modules/vscode/bin/test"
  },
  "devDependencies": {
    "@types/mocha": "^2.2.42",
    "@types/node": "^8.10.62",
    "tslint": "^5.20.1",
    "typescript": "^2.6.1",
    "vscode": "^1.1.37"
  }
}
